<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@drawable/common_item_bg"
    android:paddingLeft="@dimen/common_distance"
    android:paddingRight="@dimen/common_distance"
    android:layout_height="wrap_content">

   <TextView
       android:id="@+id/tv_createTime"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="2015/06/05 20:34"
       android:textSize="@dimen/common_identity_24px"
       android:layout_centerVertical="true"
       android:textColor="@color/light_black"
       />

    <TextView
        android:id="@+id/tv_order_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_mine_order_obligation"
        android:textSize="@dimen/common_InputBox_28px"
        android:layout_centerVertical="true"
        android:textColor="@color/light_black"
        android:layout_alignParentRight="true"
        />

</RelativeLayout>